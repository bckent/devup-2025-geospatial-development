{
    "compilerOptions": {
      "emitDecoratorMetadata": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "lib": [ "es6", "dom" ],
      "module": "ES6",
      "moduleResolution": "node",
      "noEmitOnError": true,
      "noImplicitAny": false,
      "removeComments": false,
      "sourceMap": true,
      "target": "ES2017",
      "jsx": "react-jsx",
      "allowSyntheticDefaultImports": true
    },
    "exclude": [
      "node_modules",
      "wwwroot",
      "**/*.spec.ts"
    ]
  }
  
  